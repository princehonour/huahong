/* pages/healthReport/healthReport.wxss */

.title {
  height: 70rpx;
  width: 100%;
  background-color: #E8F1F0;
  font-size: 26rpx;
  color: #5AC8B7;
  line-height: 70rpx;
  padding-left: 22rpx;
  box-sizing: border-box;
}

.question {
  padding: 0 26rpx 0 26rpx;
  width: 100%;
  box-sizing: border-box;
}

.question text {
  display: block;
  width: 100%;
  font-size: 26rpx;
  line-height: 60rpx;
}

.question view {
  background-color: #f1f3f4;
  padding: 20rpx 33rpx;
  box-sizing: border-box;
  width: 100%;
}

.btn {
  width: 100%;
  margin-top: 60rpx;
  margin-bottom: 120rpx;
}

.btn>view {
  width: 70%;
  height: 80rpx;
  background: #5AC8B7;
  border-radius: 14rpx;
  margin: 0 auto;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
}

.seecarnum text {
  display: inline-block;
}

.seecarnum text:nth-child(1) {
  width: 80%;
  float: left;
}

.seecarnum text:nth-child(2) {
  width: 20%;
  float: right;
}

.tips{
  margin-top: 40rpx;
  padding: 10rpx 30rpx;
  width: 100%;
  box-sizing: border-box;
  font-size: 26rpx;
  color:#ccc;
}

.block .van-cell__value{
  color:#000 !important;
}